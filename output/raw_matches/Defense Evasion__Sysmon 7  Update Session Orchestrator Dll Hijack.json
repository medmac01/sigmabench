{
  "title": "Unsigned DLL Loaded by Windows Utility",
  "id": "b5de0c9a-6f19-43e0-af4e-55ad01f550af",
  "description": "Detects windows utilities loading an unsigned or untrusted DLL.\nAdversaries often abuse those programs to proxy execution of malicious code.\n",
  "sigmafile": "",
  "sigma": [
    "SELECT * FROM logs WHERE Channel='Microsoft-Windows-Sysmon/Operational' AND (EventID=7 AND ((Image LIKE '%\\\\InstallUtil.exe' ESCAPE '\\' OR Image LIKE '%\\\\RegAsm.exe' ESCAPE '\\' OR Image LIKE '%\\\\RegSvcs.exe' ESCAPE '\\' OR Image LIKE '%\\\\regsvr32.exe' ESCAPE '\\' OR Image LIKE '%\\\\rundll32.exe' ESCAPE '\\') AND (NOT (Signed='true' OR (SignatureStatus='errorChaining' OR SignatureStatus LIKE 'errorCode\\_endpoint' ESCAPE '\\' OR SignatureStatus='errorExpired' OR SignatureStatus='trusted' OR SignatureStatus='Valid') OR Signed IS NULL OR (Signed='' OR Signed='-') OR SignatureStatus IS NULL OR (SignatureStatus='' OR SignatureStatus='-') OR ((Image LIKE 'C:\\\\Windows\\\\SysWOW64\\\\rundll32.exe' ESCAPE '\\' OR Image LIKE 'C:\\\\Windows\\\\System32\\\\rundll32.exe' ESCAPE '\\') AND ImageLoaded LIKE 'C:\\\\Windows\\\\Installer\\\\%' ESCAPE '\\' AND (ImageLoaded LIKE '%.tmp-\\\\Microsoft.Deployment.WindowsInstaller.dll' ESCAPE '\\' OR ImageLoaded LIKE '%.tmp-\\\\Avira.OE.Setup.CustomActions.dll' ESCAPE '\\')) OR ((Image LIKE 'C:\\\\Windows\\\\SysWOW64\\\\%' ESCAPE '\\' OR Image LIKE 'C:\\\\Windows\\\\System32\\\\%' ESCAPE '\\' OR Image LIKE 'C:\\\\Windows\\\\Microsoft.NET\\\\Framework64%' ESCAPE '\\') AND Image LIKE '%\\\\RegAsm.exe' ESCAPE '\\' AND ImageLoaded LIKE '%.dll' ESCAPE '\\' AND ImageLoaded LIKE 'C:\\\\Windows\\\\assembly\\\\NativeImages%' ESCAPE '\\'))) AND (NOT ((Image LIKE 'C:\\\\Windows\\\\SysWOW64\\\\regsvr32.exe' ESCAPE '\\' OR Image LIKE 'C:\\\\Windows\\\\System32\\\\regsvr32.exe' ESCAPE '\\') AND (ImageLoaded LIKE 'C:\\\\Program Files (x86)\\\\K-Lite Codec Pack\\\\%' ESCAPE '\\' OR ImageLoaded LIKE 'C:\\\\Program Files\\\\K-Lite Codec Pack\\\\%' ESCAPE '\\')))))"
  ],
  "rule_level": "medium",
